{
    "manifest_version": 2,
    "name": "Pomodoro",
    "description": "Pomodoro chrome extension so you dont have to keep track",
    "version": "1.0.0",
    "icons": {"128": "icon.png"},
    "browser_action": {
      "default_icon": "icon.png"
    },
    "permissions": ["tabs"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
  }
  }